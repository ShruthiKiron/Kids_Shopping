  <style>

/* Product detail page styles */
.product-detail {
    display: flex;
}
.product-images {
    flex: 1;
}
.main-image img {
    width: 50%;
    cursor: pointer;
}
.thumbnail-images {
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}
.thumbnail-images img {
    width: 30%; /* Adjust as needed */
    margin-right: 10px; /* Adjust spacing between thumbnails */
    margin-bottom: 10px; /* Adjust spacing between rows */
}
.product-info {
    flex: 2;
    padding-left: 20px;
}
.product-name {
    font-size: 24px;
    margin-top: 0;
}
.product-description {
    margin-bottom: 20px;
}
.quantity-selection select {
    padding: 5px;
    margin-bottom: 10px;
}
.action-buttons button {
    margin-right: 10px;
}
.review-section textarea {
    width: 100%;
    margin-bottom: 10px;
}
.btn {
    background-color: #2c7a64;
    color: #fff; 
    margin-right: 10px; 
}
.btn-wishlist {
    background-color: #540b0f; 
}
.product-images img {
            border: 1px solid #111010;
            cursor: pointer;
        }
        .main-image img {
            width: 60%;
        }

.out-of-stock {
    color: red;
}
.in-stock {
    color: green;
}




